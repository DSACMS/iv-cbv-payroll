<h1><%= t(".header") %></h1>
<p><%= t(".not_listed_p1") %></p>
<p><%= t(".not_listed_p2", agency_short_name: current_site.agency_short_name) %></p>
<%= link_to current_site.agency_help_link, do %>
    <button class="usa-button" type="button">
      <%= t(".exit_button", agency_short_name: current_site.agency_short_name) %>
    </button>
<% end %>
<hr class="margin-top-5 margin-bottom-5 border-base-light border-top-0">
<h2><%= t(".more_jobs") %></h2>
<p><%= t(".you_can_search") %></p>
<%= link_to cbv_flow_employer_search_path, do %>
    <button class="usa-button usa-button--outline" type="button">
      <%= t(".back_button") %>
    </button>
<% end %>