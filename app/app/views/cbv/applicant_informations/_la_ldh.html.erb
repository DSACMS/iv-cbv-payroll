<% cbv_applicant = @cbv_flow.cbv_applicant %>
<%= f.fields_for :cbv_applicant, cbv_applicant do |f2| %>
  <%= f2.memorable_date :date_of_birth, legend: t(".fields.date_of_birth.prompt"), hint: t(".fields.date_of_birth.help_text") %>
  <%= f2.text_field :case_number, label: t(".fields.case_number.prompt"), hint: t(".fields.case_number.help_text") %>
<% end %>
