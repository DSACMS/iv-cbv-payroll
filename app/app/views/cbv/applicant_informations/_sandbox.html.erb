<% cbv_applicant = @cbv_flow.cbv_applicant %>
<%= f.fields_for :cbv_applicant, cbv_applicant do |f2| %>
  <%= f2.text_field :first_name, label: t(".fields/#{@cbv_flow.client_agency_id}.first_name") %>
  <%= f2.text_field :middle_name, label: t(".fields/#{@cbv_flow.client_agency_id}.middle_name") %>
  <%= f2.text_field :last_name, label: t(".fields/#{@cbv_flow.client_agency_id}.last_name") %>
  <%= f2.text_field :case_number, label: t(".fields/#{@cbv_flow.client_agency_id}.case_number") %>
<% end %>
