<div class="field_with_errors">
  <%= f.text_field :first_name, label: t(".invite.first_name") %>
</div>

<div class="field_with_errors">
  <%= f.text_field :middle_name, label: t(".invite.middle_name") %>
</div>

<div class="field_with_errors">
  <%= f.text_field :last_name, label: t(".invite.last_name") %>
</div>

<div class="field_with_errors">
  <%= f.text_field :client_id_number, label: t(".invite.client_id_number"), hint: t(".invite.client_id_number_hint") %>
</div>

<div class="field_with_errors">
  <%= f.text_field :case_number, label: t(".invite.case_number"), hint: t(".invite.case_number_hint") %>
</div>

<div class="field_with_errors">
  <%= f.date_picker :snap_application_date, label: t(".invite.snap_application_date"), hint: t(".invite.snap_application_date_hint") %>
</div>

<div class="field_with_errors">
  <%= f.email_field :email_address, label: t(".invite.email_address") %>
</div>
