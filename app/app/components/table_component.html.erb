<%= tag.table(**@attributes.merge(class: @class_names)) do %>
  <thead class="<%= @thead_class_names %>">
    <tr>
        <%= header %>
    </tr>
    <% if subheader_row.present? %>
      <%= subheader_row %>
    <% end %>
  </thead>
  <tbody>
    <% rows.each do |row| %>
      <%= row %>
    <% end %>
  </tbody>
<% end %>