{
  "name": "app",
  "private": true,
  "type": "module",
  "dependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.16",
    "@rails/actioncable": "^7.2.202",
    "@uswds/uswds": "^3.13.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.0",
    "load-script": "^2.0.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-minify": "^1.2.0",
    "postcss-nesting": "^13.0.2",
    "postcss-scss": "^4.0.9",
    "sass": "^1.92.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "postcss ./app/assets/stylesheets/application.postcss.css -o ./app/assets/builds/application.css",
    "test": "vitest",
    "format": "prettier --write \"app/javascript/**/*.{js,jsx,ts,tsx}\" \"spec/javascript/**/*.{js,jsx,ts,tsx}\"",
    "format:precommit": "prettier --write",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "jsdom": "^24.0.0",
    "prettier": "^3.6.2",
    "vite": "~6.3.6",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "@csstools/postcss-sass/@csstools/sass-import-resolve": "https://github.com/rahearn/sass-import-resolve",
    "semver": ">= 7.5.2",
    "nanoid": "^5.0.9"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "semver": ">= 7.5.2"
  }
}
