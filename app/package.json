{
  "name": "app",
  "private": true,
  "type": "module",
  "dependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.13",
    "@rails/actioncable": "^7.2.201",
    "@uswds/uswds": "^3.12.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.0",
    "load-script": "^2.0.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "postcss-minify": "^1.1.0",
    "postcss-nesting": "^12.1.5",
    "postcss-scss": "^4.0.9",
    "sass": "^1.88.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "postcss ./app/assets/stylesheets/application.postcss.css -o ./app/assets/builds/application.css",
    "pa11y-ci": "pa11y-ci --config .pa11yci",
    "test": "vitest",
    "format": "prettier --write \"app/javascript/**/*.{js,jsx,ts,tsx}\" \"spec/javascript/**/*.{js,jsx,ts,tsx}\"",
    "format:precommit": "prettier --write"
  },
  "devDependencies": {
    "jsdom": "^24.0.0",
    "pa11y-ci": "^3.1.0",
    "prettier": "^3.5.3",
    "vite": "~6.2.7",
    "vitest": "^3.0.5"
  },
  "resolutions": {
    "@csstools/postcss-sass/@csstools/sass-import-resolve": "https://github.com/rahearn/sass-import-resolve",
    "pa11y-ci/pa11y/semver": ">= 7.5.2",
    "semver": ">= 7.5.2",
    "nanoid": "^5.0.9"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "semver": ">= 7.5.2"
  }
}
