{
  "name": "app",
  "private": true,
  "type": "module",
  "dependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.21",
    "@rails/actioncable": "^8.1.200",
    "@uswds/uswds": "^3.13.0",
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.27.0",
    "load-script": "^2.0.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-minify": "^1.2.0",
    "postcss-nesting": "^14.0.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.97.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "build:css": "postcss ./app/assets/stylesheets/application.postcss.css -o ./app/assets/builds/application.css",
    "test": "vitest",
    "format": "prettier --write \"app/javascript/**/*.{js,jsx,ts,tsx,json}\" \"spec/javascript/**/*.{js,jsx,ts,tsx,json}\"",
    "format:precommit": "prettier --write"
  },
  "devDependencies": {
    "happy-dom": ">=20.3.3",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "resolutions": {
    "@csstools/postcss-sass/@csstools/sass-import-resolve": "https://github.com/rahearn/sass-import-resolve",
    "nanoid": "^5.0.9"
  },
  "overrides": {
    "esbuild": "^0.27.0"
  }
}
